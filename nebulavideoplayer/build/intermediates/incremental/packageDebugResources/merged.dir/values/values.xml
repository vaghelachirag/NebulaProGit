<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="jz_start_button_w_h_fullscreen">62dp</dimen>
    <dimen name="jz_start_button_w_h_normal">45dp</dimen>
    <item name="jz_fullscreen_id" type="id"/>
    <item name="jz_tiny_id" type="id"/>
    <string name="no_url">No url</string>
    <string name="replay">Replay</string>
    <string name="tips_not_wifi">You are currently using the mobile network, the player will continue to consume traffic</string>
    <string name="tips_not_wifi_cancel">Stop play</string>
    <string name="tips_not_wifi_confirm">Resume</string>
    <style name="AVLoadingIndicatorView">
        <item name="minWidth">18dp</item> <!--48 dip-->
        <item name="maxWidth">18dp</item>
        <item name="minHeight">18dp</item>
        <item name="maxHeight">18dp</item>
        <item name="indicatorName">BallPulseIndicator</item>
    </style>
    <style name="AVLoadingIndicatorView.Small">
        <item name="minWidth">14dp</item><!--24 dip-->
        <item name="maxWidth">14dp</item>
        <item name="minHeight">14dp</item>
        <item name="maxHeight">14dp</item>
        <item name="indicatorName">BallPulseIndicator</item>
    </style>
    <style name="jz_popup_toast_anim" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="jz_style_dialog_progress" parent="@android:style/Theme.Dialog">
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowAnimationStyle">@style/jz_popup_toast_anim</item>
    <item name="android:backgroundDimEnabled">false</item>
</style>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr format="dimension" name="minWidth"/>
        <attr format="dimension" name="maxWidth"/>
        <attr format="dimension" name="minHeight"/>
        <attr format="dimension" name="maxHeight"/>
        <attr format="string" name="indicatorName"/>
        <attr format="color" name="indicatorColor"/>
    </declare-styleable>
</resources>