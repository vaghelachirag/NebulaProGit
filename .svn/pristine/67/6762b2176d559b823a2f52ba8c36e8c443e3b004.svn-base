<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ln_boos_pay_detials"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/dimen_one_dp"
    android:layout_marginEnd="@dimen/dimen_three_dp"
    android:layout_marginLeft="@dimen/dimen_three_dp"
    android:layout_marginRight="@dimen/dimen_three_dp"
    android:layout_marginStart="@dimen/dimen_three_dp"
    android:layout_marginTop="@dimen/dimen_one_dp"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:visibility="gone">

    <LinearLayout
        android:id="@+id/boos_pay_detials"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_thirty_dp"
        android:layout_marginBottom="@dimen/dimen_five_dp"
        android:layout_marginEnd="@dimen/dimen_three_dp"
        android:layout_marginLeft="@dimen/dimen_three_dp"
        android:layout_marginRight="@dimen/dimen_three_dp"
        android:layout_marginStart="@dimen/dimen_three_dp"
        android:layout_marginTop="@dimen/dimen_five_dp"
        android:background="@drawable/table_effect"
        android:orientation="horizontal"
        android:visibility="gone">

        <com.nebulacompanies.ibo.view.MyTextView
            android:id="@+id/booster_income_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:gravity="center"
            android:padding="8dp"
            android:textAlignment="gravity"
            android:textColor="@android:color/white"
            android:textSize="@dimen/dimen_fourteen_sp" />
        <!--<ImageView
            android:id="@+id/booster_icon"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="0.1"
            android:layout_gravity="center"
            android:src="@drawable/ic_keyboard_arrow_down_white_24dp"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dimen_one_dp"
        android:layout_marginEnd="@dimen/dimen_three_dp"
        android:layout_marginLeft="@dimen/dimen_three_dp"
        android:layout_marginRight="@dimen/dimen_three_dp"
        android:layout_marginStart="@dimen/dimen_three_dp"
        android:layout_marginTop="@dimen/dimen_one_dp"
        android:background="@drawable/table_effect"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/booster_payout_title"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_thirty_dp"
            android:orientation="horizontal"
            android:visibility="gone"
            android:weightSum="3">

            <com.nebulacompanies.ibo.view.MyTextView
                android:id="@+id/title_one_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:singleLine="false"
                android:text="Sr"
                android:textColor="#ffffff"
                android:textSize="@dimen/dimen_fourteen_sp" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#cfcfcf" />

            <com.nebulacompanies.ibo.view.MyTextView
                android:id="@+id/title_two_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:singleLine="false"
                android:text="@string/date"
                android:textColor="#ffffff"
                android:textSize="@dimen/dimen_fourteen_sp" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#cfcfcf" />

            <com.nebulacompanies.ibo.view.MyTextView
                android:id="@+id/title_three_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:singleLine="false"
                android:text="Payout"
                android:textColor="#ffffff"
                android:textSize="@dimen/dimen_fourteen_sp" />
        </LinearLayout>

        <ListView
            android:id="@+id/booster_payout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_two_dp"
            android:background="@drawable/drawable_rounded"
            android:divider="@null"
            android:dividerHeight="0dp"
            android:fadeScrollbars="false"
            android:layoutAnimation="@anim/layout_animation_fall_down"
            android:padding="@dimen/dimen_two_dp"
            android:smoothScrollbar="true" />
    </LinearLayout>
</LinearLayout>