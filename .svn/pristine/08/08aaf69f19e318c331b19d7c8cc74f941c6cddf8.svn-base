<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="otpViewStyle">@style/OtpWidget.OtpView</item>
    </style>

    <style name="Button" parent="Base.TextAppearance.AppCompat">
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="CheckboxStyle" parent="AppTheme">
        <item name="colorAccent">@color/nebula</item>
        <item name="android:textColorSecondary">@color/nebula</item>

    </style>

    <style name="Toolbar_text" parent="Base.TextAppearance.AppCompat">
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimen_fourteen_sp</item>
    </style>

    <style name="Tabs" parent="Base.TextAppearance.AppCompat">
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>


    </style>

    <style name="Charts" parent="Base.TextAppearance.AppCompat">
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">@dimen/dimen_twenty_sp</item>
        <item name="android:fontFamily">fonts/Montserrat-Regular.ttf</item>
        <item name="android:clickable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:focusable">false</item>

    </style>

    <style name="Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:textColor">@color/nebula</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>


    <style name="SelectableItemTheme">
        <item name="colorControlHighlight">#BDBDBD</item>
    </style>

    <style name="SelectableItemBackground">
        <item name="android:theme">@style/SelectableItemTheme</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="StartUp" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- This disables the black preview screen -->
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="CustomRatingBarStyle" parent="android:style/Widget.Material.RatingBar.Small">
        <item name="android:progressDrawable">@drawable/custom_rating</item>
        <item name="android:minHeight">15dip</item>
        <item name="android:maxHeight">47dip</item>
        <item name="android:padding">0dp</item>
    </style>


    <style name="Update_Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:textColor">@color/nebula</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
    </style>

    <style name="DialogAnimationFade">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

    <style name="DatePickerDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorControlNormal">@android:color/white</item>
        <item name="colorControlActivated">@color/vacation_bg</item>
        <item name="textColorAlertDialogListItem">@android:color/white</item>
        <item name="colorAccent">@color/vacation_bg</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:windowBackground">@drawable/drawable_date_picker_theme</item>
    </style>


    <style name="StyleToastWhile">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:colorBackground">@color/vacation_bg</item>
        <item name="android:strokeWidth" tools:targetApi="lollipop">@color/vacation_bg</item>
        <item name="android:strokeColor" tools:targetApi="lollipop">@color/vacation_bg</item>
        <item name="android:radius">@dimen/dimen_ten_dp</item>
        <item name="android:icon">@drawable/ic_error_new</item>
    </style>


    <!--//ContactUs-->

    <style name="ToolBarStyle" parent="Theme.AppCompat">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:fontFamily">@font/montserrat_regular</item>
        <item name="actionMenuTextColor">@android:color/white</item>
    </style>

    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
