        <item name="android:textSize">20sp</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
    </style>

    <style name="CustomText2" parent="TextAppearance.ShowcaseView.Detail.Light">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:fontFamily">@font/montserrat_regular</item>
        <item name="android:textSize">@dimen/dimen_sixteen_sp</item>
        <item name="actionMenuTextColor">@android:color/white</item>
    </style>

    <!--Show Case View Back-->

    <style name="ShowcaseButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">96dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">@drawable/nebula_button_login_eftct_customer_login</item>
    </style>

