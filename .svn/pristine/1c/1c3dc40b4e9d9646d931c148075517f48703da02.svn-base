                    android:layout_marginEnd="@dimen/dimen_eight_dp"
                    android:layout_marginBottom="@dimen/dimen_four_dp"
                    android:clickable="true"
                    android:elevation="3dp"
                    android:focusable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardBackgroundColor="@android:color/white">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen_five_dp"
                            android:gravity="center"
                            android:text="Share Your Referral Link"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/dimen_fourteen_dp"
                            android:textStyle="bold" />


                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen_ten_dp"
                            app:srcCompat="@drawable/ic_gift_box" />

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/dimen_ten_dp"
                            android:layout_marginTop="@dimen/dimen_five_dp"
                            android:layout_marginEnd="@dimen/dimen_ten_dp"
                            android:gravity="center"
                            android:padding="@dimen/dimen_ten_dp"
                            android:text="@string/referraldetails"
                            android:textColor="@android:color/darker_gray"
                            android:textSize="@dimen/dimen_fourteen_dp"
                            android:textStyle="normal" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tvref_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/dimen_ten_dp"
                                android:layout_marginTop="@dimen/dimen_five_dp"
                                android:layout_marginBottom="@dimen/dimen_five_dp"
                                android:layout_weight="0.80"
                                android:background="@drawable/round_textview"
                                android:gravity="center"
                                android:padding="@dimen/dimen_ten_dp"
                                android:text=""
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/dimen_fourteen_dp"
                                android:textStyle="bold" />

                            <com.nebulacompanies.ibo.ecom.utils.AspectRatioImageView
                                android:id="@+id/img_share"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/dimen_four_dp"
                                android:layout_marginEnd="@dimen/dimen_four_dp"
                                android:layout_weight="0.20"
                                android:gravity="start|center"
                                android:padding="@dimen/dimen_five_dp"
                                android:src="@drawable/ic_share"
                                android:visibility="visible" />

                        </LinearLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                </LinearLayout>

            </ScrollView>



