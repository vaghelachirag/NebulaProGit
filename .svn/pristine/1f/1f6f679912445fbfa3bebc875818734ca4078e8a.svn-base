<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/list_income_details_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/nebula_effect"
        android:orientation="horizontal"
        android:weightSum="12">

        <com.nebulacompanies.ibo.view.MyTextView
            android:id="@+id/date_starclub1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="4"
            android:gravity="center"
            android:padding="@dimen/dimen_eight_dp"
            android:textColor="#000000"
            android:textSize="@dimen/dimen_twelve_sp" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#cfcfcf" />

        <com.nebulacompanies.ibo.view.MyTextView
            android:id="@+id/rank_starpool1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="4.5"
            android:gravity="center"
            android:padding="@dimen/dimen_eight_dp"
            android:textColor="#000000"
            android:textSize="@dimen/dimen_twelve_sp" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#cfcfcf" />

        <com.nebulacompanies.ibo.view.MyTextView
            android:id="@+id/gross_amt_starpool1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="3.5"
            android:gravity="center"
            android:padding="@dimen/dimen_eight_dp"
            android:textColor="#000000"
            android:textSize="@dimen/dimen_twelve_sp" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="#cfcfcf" />

        <ImageView
            android:id="@+id/more_info_starpool1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="2"
            android:background="@drawable/nebula_effect"
            android:gravity="center"
            android:padding="5dp"
            android:visibility="gone"
            android:src="@drawable/ic_info_black" />

        <!--<TextView
            android:id="@+id/more_info_starpool1"
            android:layout_width="0dp"
            android:layout_height = "wrap_content"
            android:layout_weight="3"
            android:gravity = "center"
            android:padding = "5dp"
            android:text="@string/info"
            android:textColor = "#000000"
            android:layout_gravity="center"/>-->


    </LinearLayout>

</LinearLayout>