                                android:layout_gravity="start"
                                android:layout_marginTop="@dimen/dimen_two_dp"
                                android:gravity="start"
                                android:text=""
                                android:textColor="@color/colorOrderSummary"
                                android:textSize="@dimen/dimen_fourteen_sp"
                                android:visibility="gone" />


                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_shipping_address_mobile"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tv_shipping_address_four"
                                android:layout_gravity="start"
                                android:layout_marginTop="@dimen/dimen_two_dp"
                                android:gravity="start"
                                android:text=""
                                android:textColor="@color/colorOrderSummary"
                                android:textSize="@dimen/dimen_fourteen_sp"
                                android:visibility="gone" />


                        </RelativeLayout>

                        <View
