<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_ordersummary"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_layout_search"
        android:visibility="visible" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/ns_order_summary"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/btn_confirm_order"
        android:layout_below="@+id/toolbar"
        android:layout_marginStart="@dimen/dimen_six_dp"
        android:layout_marginEnd="@dimen/dimen_six_dp"
        android:visibility="visible">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/dimen_six_dp"
                android:layout_marginBottom="@dimen/dimen_four_dp"
                android:clickable="true"
                android:elevation="3dp"
                android:focusable="true"
                android:visibility="visible"
                app:cardBackgroundColor="@android:color/white">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_ten_dp"
                        android:layout_marginTop="@dimen/dimen_five_dp"
                        android:layout_marginEnd="@dimen/dimen_ten_dp"
                        android:orientation="horizontal"
                        android:visibility="visible">

                        <com.nebulacompanies.ibo.ecom.utils.MyBoldTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:text="Total Items"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen_fourteen_sp" />


                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:id="@+id/tv_order_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginEnd="@dimen/dimen_four_dp"
                            android:gravity="center"
                            android:paddingTop="@dimen/dimen_two_dp"
                            android:paddingBottom="@dimen/dimen_two_dp"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen_fourteen_sp"
                            android:textStyle="normal" />

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:id="@+id/tv_order_id_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:paddingTop="@dimen/dimen_two_dp"
                            android:paddingBottom="@dimen/dimen_two_dp"
                            android:text=""
                            android:textColor="@color/holo_red_dark"
                            android:textSize="@dimen/dimen_sixteen_sp"
                            android:textStyle="normal" />


                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/lay_wallet"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dimen_one_dp"
                            android:layout_marginStart="@dimen/dimen_ten_dp"
                            android:layout_marginEnd="@dimen/dimen_ten_dp"
                            android:background="@color/gray" />

                        <CheckBox
                            android:id="@+id/cbWalletval"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_ten_dp"
                            android:layout_marginEnd="@dimen/dimen_ten_dp"
                            android:checked="true"
                            android:text="Use ₹1000 of your ₹2000 E-wallet balance."
                            android:textColor="@color/black"

                            app:theme="@style/CheckboxStyle" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_one_dp"
                        android:layout_marginStart="@dimen/dimen_ten_dp"
                        android:layout_marginEnd="@dimen/dimen_ten_dp"
                        android:background="@color/gray" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_ten_dp"
                        android:paddingTop="@dimen/dimen_five_dp"
                        android:layout_marginEnd="@dimen/dimen_ten_dp"
                        android:paddingBottom="@dimen/dimen_five_dp"
                        android:orientation="horizontal">

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:text="Subtotal"

                            android:textColor="@color/colorOrderSummary"
                            android:textSize="@dimen/dimen_fourteen_sp" />


                      <!--  <com.nebulacompanies.ibo.ecom.utils.MyBoldTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="end"
                            android:text="Subtotal"
                            android:textColor="@color/colorOrderSummary"
                            android:textSize="@dimen/dimen_fourteen_sp"
                            android:visibility="visible" />-->


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight=".7"
                            android:gravity="end"
                            android:orientation="horizontal">


                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_sub_total_symbol"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="top"
                                android:layout_marginTop="@dimen/dimen_two_dp"
                                android:layout_toStartOf="@+id/tv_order_subtotal_value"
                                android:paddingTop="@dimen/dimen_two_dp"
                                android:paddingBottom="@dimen/dimen_two_dp"
                                android:text="₹"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/dimen_twelve_sp"
                                android:textStyle="normal" />

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_subtotal_value"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_gravity="start"
                                android:paddingTop="@dimen/dimen_two_dp"
                                android:paddingBottom="@dimen/dimen_two_dp"
                                android:text=""
                                android:textColor="@color/holo_red_dark"
                                android:textSize="@dimen/dimen_sixteen_sp"
                                android:textStyle="normal" />

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
