                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dimen_ten_dp"
                        android:id="@+id/laywalletvalue"
                        android:visibility="gone"
                        android:paddingBottom="@dimen/dimen_five_dp"
                        android:orientation="horizontal">

                        <!--<com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:text="Wallet Discount"
                            android:textColor="@color/colorOrderSummary"
                            android:textSize="@dimen/dimen_fourteen_sp" />-->

                        <CheckBox
                            android:id="@+id/cbWallet"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:scaleX="0.80"
                            android:scaleY="0.80"
                            android:gravity="start|center"
                            android:checked="true"
                            android:text=""
                            android:textColor="@color/colorOrderSummary"
                            android:layout_marginTop = "-5dp"
                            android:layout_marginBottom= "-5dp"
                            app:theme="@style/CheckboxStyle" />

                      <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
