                            android:textSize="@dimen/dimen_fourteen_sp" />


                      <!--  <com.nebulacompanies.ibo.ecom.utils.MyBoldTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="end"
                            android:text=""
                            android:textColor="@color/colorOrderSummary"
                            android:textSize="@dimen/dimen_fourteen_sp"
                            android:visibility="visible" />-->

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight=".7"
                            android:gravity="end"
                            android:orientation="horizontal">

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_shipping_charge_symbol"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="top"
                                android:layout_marginTop="@dimen/dimen_two_dp"
                                android:paddingTop="@dimen/dimen_two_dp"
                                android:paddingBottom="@dimen/dimen_two_dp"
                                android:text="₹"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/dimen_twelve_sp"
                                android:textStyle="normal" />

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_shiping_price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_gravity="start"
                                android:paddingTop="@dimen/dimen_two_dp"
                                android:paddingBottom="@dimen/dimen_two_dp"
                                android:text=""
                                android:textColor="@color/holo_red_dark"
                                android:textSize="@dimen/dimen_sixteen_sp"
                                android:textStyle="normal" />


                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_ten_dp"
                        android:layout_marginEnd="@dimen/dimen_ten_dp"
                        android:paddingBottom="@dimen/dimen_five_dp"
                        android:orientation="horizontal">

                        <com.nebulacompanies.ibo.ecom.utils.MyBoldTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:text="Grand Total"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen_fourteen_sp" />


                       <!-- <com.nebulacompanies.ibo.ecom.utils.MyBoldTextViewEcom
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="end"
                            android:text="Grand Total"
                            android:textColor="@color/colorOrderSummary"
                            android:textSize="@dimen/dimen_fourteen_sp" />-->

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight=".7"
                            android:gravity="end"
                            android:orientation="horizontal">

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_grand_total_symbol"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="top"
                                android:layout_marginTop="@dimen/dimen_two_dp"
                                android:layout_toStartOf="@+id/tv_order_grand_total_value"
                                android:paddingTop="@dimen/dimen_two_dp"
                                android:paddingBottom="@dimen/dimen_two_dp"
                                android:text="₹"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/dimen_twelve_sp"
                                android:textStyle="normal" />

                            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                                android:id="@+id/tv_order_grand_total_value"
